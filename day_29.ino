void draw(void) {
  // graphic commands to redraw the complete screen should be placed here  
  u8g.setFont(u8g_font_unifont);
 
  CoordinateKeypadCheck();
 
  if(System_Error == 1) {
    displayBitmap = 10;
  }
 
  switch (displayBitmap) {
      case 0:
        u8g.drawXBMP( 0, 0, 128, 64, defaultFace);
        break;
      case 1:
